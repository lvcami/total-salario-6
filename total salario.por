programa {
  funcao inicio() {
    real salario, totalsalario=0

    para(inteiro cont=1; cont<=6; cont++) {

      escreva("digite o salario do funcionario: ")
      leia(salario)

      totalsalario=totalsalario+salario 
    }
    escreva("O salario do funcionário é: ", totalsalario)
    
    
    
  }
}
